{
    "name": "craftisan/laravel-api-tester",
    "description": "Api tester for Laravel Framework",
    "keywords": [
        "laravel",
        "api",
        "debug",
        "laravel api tester"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Deekshant Joshi",
            "email": "deekshant.joshi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^7.0",
        "ktamas77/firebase-php": "^3.0",
        "firebase/token-generator": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3.1"
    },
    "suggest": {
        "ktamas77/firebase-php": "Keep your requests stored in cloud",
        "firebase/token-generator": "Token generator for firebase"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "Craftisan\\ApiTester\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Craftisan\\ApiTester\\ServiceProvider"
            ]
        }
    }
}
